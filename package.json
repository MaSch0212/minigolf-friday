{
  "name": "minigolf-friday-repo",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "preinstall": "pnpm install -C client && dotnet restore",
    "build": "pnpm server:build && pnpm client:build && pnpm docker:build",
    "push": "pnpm docker:push",
    "server:build": "dotnet publish server -c Release",
    "client:build": "pnpm run -C client build",
    "docker:build": "node scripts/docker-build.js",
    "docker:build:inttest": "node scripts/docker-build-inttest.js",
    "docker:push": "node scripts/docker-push.js",
    "test:integration": "dotnet test test/MinigolfFriday.IntegrationTests/MinigolfFriday.IntegrationTests.csproj"
  }
}
